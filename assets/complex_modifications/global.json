{
  "title": "Global key bindings",
  "rules": [
    {
      "description": "global: caps as command",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "caps_lock"
          },
          "to": [
            {
              "key_code": "left_command"
            }
          ]
        }
      ]
    }
  ]
}
